{
  "menu-item#categories-ropa": {
    "blocks": ["vtex.menu@2.x:submenu#categories-ropa"],
    "props": {
      "blockClass": "categories",
      "itemProps": {
        "text": "Seasons",
        "href": "/seasons"
      }
    }
  },
  "vtex.menu@2.x:submenu#categories-ropa": {
    "children": ["flex-layout.row#categories-ropa"],
    "props": {
      "width": "100%",
      "blockClass": "subcategories"
    }
  },
  "flex-layout.row#categories-ropa": {
    "children": [
      "flex-layout.col#categories-ropa-items",
      "flex-layout.col#categories-ropa-images"
    ],
    "props": {
      "preventHorizontalStretch": true,
      "colGap": 9,
      "blockClass": "subcategories"
    }
  },

  "flex-layout.col#categories-ropa-items": {
    "children": ["flex-layout.row#categories-ropa-items"],
    "props": {
      "horizontalAlign": "center"
    }
  },
  "flex-layout.row#categories-ropa-items": {
    "children": [
      "vtex.menu@2.x:menu#subcategory-para-dormir",
      "vtex.menu@2.x:menu#subcategory-para-el-frio",
      "vtex.menu@2.x:menu#subcategory-regreso-clases"
    ],
    "props": {
      "preventHorizontalStretch": true,
      "colGap": 0,
      "blockClass": "subcategories-items"
    }
  },
  "vtex.menu@2.x:menu#subcategory-ropa-varios": {
    "children": [
      "menu-item#subcategory-ropa-active",
      "menu-item#subcategory-ropa-Bikinis",
      "menu-item#subcategory-ropa-tyedye",
      "menu-item#subcategory-ropa-pantuflas",
      "menu-item#subcategory-ropa-interior",
      "menu-item#subcategory-ropa-homewear"
    ],
    "props": {
      "orientation": "vertical",
      "blockClass": "subcategory-item"
    }
  },
  "menu-item#subcategory-ropa-active": {
    "props": {
      "blockClass": "subcategory-title",
      "itemProps": { "text": "Active" }
    }
  },
  "menu-item#subcategory-ropa-Bikinis": {
    "props": {
      "blockClass": "subcategory-title",
      "itemProps": { "text": "Bikinis" }
    }
  },
  "menu-item#subcategory-ropa-tyedye": {
    "props": {

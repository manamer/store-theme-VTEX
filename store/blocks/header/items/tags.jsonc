      "blockClass": ["full", "header-tags", "header-tags-mobile"],
      "horizontalAlign": "center",
      "preventHorizontalStretch": true,
      "preventVerticalStretch": true,
      "fullWidth": true
    },
    "children": ["vtex.menu@2.x:menu#header-tags-mobile"]

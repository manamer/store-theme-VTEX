{
  "flex-layout.row#header-tags": {
    "title": "Tags",
    "props": {
      "blockClass": ["full", "header-tags"],
      "horizontalAlign": "center",
      "preventHorizontalStretch": true,
      "preventVerticalStretch": true,
      "fullWidth": true
    },
    "children": [
      "vtex.menu@2.x:menu#header-tags-left",
      "flex-layout.col#spacer",
      "vtex.menu@2.x:menu#header-tags-right"
    ]
  },
  "vtex.menu@2.x:menu#header-tags-left": {
    "children": [
      "menu-item#header-tag-safe-purchase",
      "menu-item#header-tag-whatsapp"
    ]
  },
  "vtex.menu@2.x:menu#header-tags-right": {
    "children": [
      "menu-item#header-tag-stores",
      "menu-item#header-tag-contact-us"
    ]
  },

  "flex-layout.row#header-tags-mobile": {
    "title": "Tags Mobile",
